{
  "name": "fhevm-react-template",
  "version": "2.0.0",
  "description": "Universal FHEVM SDK - Framework-agnostic toolkit for building privacy-preserving dApps with Fully Homomorphic Encryption",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "install:all": "npm install && npm run bootstrap",
    "bootstrap": "npm install --workspaces",
    "build": "npm run build --workspaces --if-present",
    "build:sdk": "npm run build --workspace=packages/fhevm-sdk",
    "dev:nextjs": "npm run dev --workspace=examples/nextjs-anonymous-court",
    "dev:court": "npm run dev --workspace=examples/anonymous-court-investigation",
    "start:nextjs": "npm run dev:nextjs",
    "compile:contracts": "npm run compile --workspaces --if-present",
    "deploy:contracts": "npm run deploy --workspaces --if-present",
    "test": "npm test --workspaces --if-present",
    "test:sdk": "npm test --workspace=packages/fhevm-sdk",
    "lint": "npm run lint --workspaces --if-present",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "npm run clean --workspaces --if-present && rm -rf node_modules"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "zama",
    "homomorphic-encryption",
    "privacy",
    "blockchain",
    "ethereum",
    "sdk",
    "react",
    "nextjs",
    "wagmi",
    "web3"
  ],
  "author": "FHEVM SDK Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IrwinDenesik/fhevm-react-template.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "prettier": "^3.3.0",
    "typescript": "^5.9.3"
  }
}
